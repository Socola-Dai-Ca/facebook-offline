{
    "name": "Facebook Offline",
    "version": "1.0",
    "author": "MrTaiw",
    "description": "Disable status online, last active, seen message.",
    "homepage_url": "https://thucdem.mobi",
    "permissions": ["<all_urls>", "webRequest", "webRequestBlocking", "storage"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
	    "128": "icon.png"
	},
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}